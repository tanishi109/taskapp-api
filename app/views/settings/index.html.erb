<h1>Setting</h1>

<% @settings.each do |setting| %>
  <div>
    <ul>
      <li>
        <%= link_to "Setting: "+setting.name, setting_path(setting.id) %>
      </li>
      <li>
        <%= link_to "編集", edit_setting_path(setting.id) %>
        <%= link_to "削除", setting_path(setting.id), method: :delete, data: { confirm: "are you sure?" } %>
      </li>
    </ul>
  </div>
<% end %>

<p><%= link_to "追加", new_setting_path %><p>