<h1>Projects</h1>

<% @projects.each do |project| %>
  <div>
    <ul>
      <li>
        title: <%= link_to project.title, project_path(project.id) %>
      </li>
      <li>
        detail: <%= project.detail %>
      </li>
      <li>
        finished: <%= project.finished %>
      </li>
      <li>
        <%= link_to "削除", project_path(project.id), method: :delete, data: { confirm: "are you sure?" } %>
      </li>
    </ul>
  </div>
<% end %>

<p><%= link_to "追加", new_project_path %><p>